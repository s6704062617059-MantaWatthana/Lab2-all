#include <stdio.h>

int main() {
    int a, b, c, d;
    int max1, max2;

    scanf("%d %d %d %d", &a, &b, &c, &d);

    // หา max1
    max1 = (a > b) ? a : b;
    max1 = (max1 > c) ? max1 : c;
    max1 = (max1 > d) ? max1 : d;

    // หา max2
    max2 = (a < max1) ? a : -2147483648;
    max2 = (b < max1 && b > max2) ? b : max2;
    max2 = (c < max1 && c > max2) ? c : max2;
    max2 = (d < max1 && d > max2) ? d : max2;

    printf("%d\n", max2);
    return 0;
}
